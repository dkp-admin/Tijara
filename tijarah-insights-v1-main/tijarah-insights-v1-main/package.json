    "formik": "^2.4.5",
    "i18n-js": "^3.9.2",
    "prop-types": "^15.8.1",
    "qs": "^6.11.2",
    "react": "18.2.0",
    "react-native": "0.72.10",
    "react-native-event-listeners": "^1.0.7",
