    "build:test": "APP_ENV=test && npx eas-cli build --profile test --platform android --local",
    "build:qa-ios": "APP_ENV=qa && npx eas-cli build --profile qa --platform ios --local",
    "build:prod": "APP_ENV=production && npx eas-cli build --profile production --platform android --local",
    "build:prod-ios": "APP_ENV=production && npx eas-cli build --profile production --platform ios --local",
    "i18-scan": "npx i18next-scanner",
    "translate": "node scripts/localization-script.js",
    "generate-unique-csv": "node ./scripts/generate-unique.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
