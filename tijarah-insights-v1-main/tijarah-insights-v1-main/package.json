{
  "name": "merchant-analytics",
  "version": "1.0.2",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:qa": "APP_ENV=qa && npx eas-cli build --profile qa --platform android --local",
